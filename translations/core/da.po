"    P = vis produktion\n"
"    A = vis landareal\n"
"    L = vis læsekyndighed\n"
"    R = vis forskningshastighed\n"
"    S = vis befolket areal\n"
"    E = vis økonomi\n"
"    M = vis militærtjeneste\n"
"    R = vis forskningshastighed\n"
"    O = vis forurening\n"
